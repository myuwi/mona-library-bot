{
  "name": "mona-library-bot",
  "version": "0.14.0",
  "description": "Mona Library Discord Bot",
  "author": "Myuwi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production node dist/bot.js",
    "start:dev": "cross-env NODE_ENV=development node dist/bot.js",
    "dev": "cross-env NODE_ENV=development ts-node src/bot.ts",
    "dev:r": "cross-env NODE_ENV=development ts-node-dev src/bot.ts",
    "lint": "eslint . --ext .ts",
    "eslint-fix": "eslint . --fix --ext .ts",
    "check": "tsc --noEmit -p .",
    "build": "tsc -p .",
    "generate-names": "ts-node src/generate-names.ts",
    "test": "jest -i"
  },
  "dependencies": {
    "@googleapis/docs": "^2.0.1",
    "@prisma/client": "^4.12.0",
    "bufferutil": "^4.0.6",
    "chalk": "4.1.2",
    "common-tags": "^1.8.2",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.0",
    "fuse.js": "^6.5.3",
    "prisma": "^4.12.0",
    "sharp": "^0.32.0",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
}
