{
    "name": "mona-library-bot",
    "version": "0.2.1",
    "description": "Mona Library Discord Bot",
    "author": "Myuwi",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start:prod": "cross-env NODE_ENV=production node dist/bot.js",
        "start:dev": "cross-env NODE_ENV=development node dist/bot.js",
        "dev": "cross-env NODE_ENV=development ts-node src/bot.ts",
        "dev:r": "cross-env NODE_ENV=development ts-node-dev src/bot.ts",
        "eslint-fix": "eslint . --fix --ext .ts",
        "build": "tsc -p .",
        "knex:migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
        "knex:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
        "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
        "generate-names": "ts-node src/NamesToImages.ts",
        "test": "jest"
    },
    "dependencies": {
        "@googleapis/docs": "^0.2.0",
        "bufferutil": "^4.0.3",
        "chalk": "^4.1.1",
        "common-tags": "^1.8.0",
        "discord.js": "^13.0.1",
        "dotenv": "^9.0.1",
        "knex": "^0.95.11",
        "sharp": "^0.29.0",
        "sqlite3": "^5.0.2",
        "text-to-image": "^4.1.1",
        "utf-8-validate": "^5.0.5",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.0",
        "@types/jest": "^27.0.3",
        "@types/node": "^15.0.2",
        "@types/sharp": "^0.28.6",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.26.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "jest": "^27.3.1",
        "prettier": "2.5.0",
        "ts-jest": "^27.0.7",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
    }
}
