{
  "name": "mona-library-bot",
  "version": "0.15.0",
  "description": "Mona Library Discord Bot",
  "author": "Myuwi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "cross-env NODE_ENV=production node dist",
    "start:dev": "cross-env NODE_ENV=development node dist",
    "dev": "cross-env NODE_ENV=development ts-node src",
    "dev:watch": "cross-env NODE_ENV=development ts-node-dev src",
    "lint": "eslint . --ext .ts",
    "eslint-fix": "eslint . --fix --ext .ts",
    "check": "tsc --noEmit -p .",
    "build": "tsc -p .",
    "generate-names": "ts-node src/generate-names.ts",
    "test": "jest -i",
    "register": "ts-node src/register.ts"
  },
  "dependencies": {
    "@googleapis/docs": "^2.0.1",
    "bufferutil": "^4.0.6",
    "chalk": "4.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.0",
    "fuse.js": "^6.5.3",
    "sharp": "^0.32.1",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "type-fest": "^3.11.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  }
}
