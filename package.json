{
    "name": "mona-library-bot",
    "version": "0.4.2",
    "description": "Mona Library Discord Bot",
    "author": "Myuwi",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start:prod": "cross-env NODE_ENV=production node dist/bot.js",
        "start:dev": "cross-env NODE_ENV=development node dist/bot.js",
        "dev": "cross-env NODE_ENV=development ts-node src/bot.ts",
        "dev:r": "cross-env NODE_ENV=development ts-node-dev src/bot.ts",
        "eslint-fix": "eslint . --fix --ext .ts",
        "build": "tsc -p .",
        "knex:migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
        "knex:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
        "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
        "generate-names": "ts-node src/NamesToImages.ts",
        "test": "jest"
    },
    "dependencies": {
        "@googleapis/docs": "^0.2.0",
        "bufferutil": "^4.0.6",
        "chalk": "4.1.2",
        "common-tags": "^1.8.2",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "fuse.js": "^6.5.3",
        "knex": "^0.95.15",
        "sharp": "^0.30.3",
        "sqlite3": "^5.0.2",
        "text-to-image": "^5.2.0",
        "utf-8-validate": "^5.0.9",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.1",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@types/sharp": "^0.30.0",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^27.5.1",
        "prettier": "2.6.1",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.3"
    }
}
