{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "es2019",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "types": [
            "jest",
            "node",
            "discord.js"
        ],
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "resolveJsonModule": false
    },
    "include": [
        "src/**/*.ts"
    ],
    "compileOnSave": true
}